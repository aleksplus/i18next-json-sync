{
  "name": "i18next-json-sync",
  "version": "0.0.1",
  "description": "keep i18next json resource files in sync",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "bin": {
    "sync-i18n": "dist/cli.js"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p src && tsc -p testRunner --noEmit",
    "ci": "npm run lint && npm run build && npm run coverage",
    "coverage": "rimraf coverage && npm run test -- --coverage",
    "lint": "tslint src/**/*.ts testRunner/**/*.ts",
    "pretest": "rimraf tests/**/testRunner.js && ts-node -P testRunner testRunner/testInit",
    "reset": "git clean -xdf -e\"node_modules\" && npm run typings",
    "test": "jest --config jest.json",
    "typings": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jwbay/i18next-json-sync.git"
  },
  "keywords": [
    "i18next",
    "json",
    "sync"
  ],
  "author": "justin.bay@outlook.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jwbay/i18next-json-sync/issues"
  },
  "homepage": "https://github.com/jwbay/i18next-json-sync#readme",
  "devDependencies": {
    "jest": "^15.1.1",
    "rimraf": "^2.5.4",
    "shelljs": "^0.7.3",
    "ts-node": "^1.2.2",
    "tslint": "^3.15.1",
    "tslint-microsoft-contrib": "^2.0.9",
    "tslint-misc-rules": "^2.6.2",
    "typescript": "^2.0.3",
    "typings": "^1.3.2"
  },
  "dependencies": {
    "glob": "^7.0.5",
    "json-stable-stringify": "^1.0.1",
    "yargs": "^5.0.0"
  }
}
